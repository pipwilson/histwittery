{
  "title":{
    "text":  "Twitter posting frequency",
    "style": "{font-size: 20px; color:#0000ff; font-family: Verdana; text-align: center;}"
  },
 
  "y_legend":{
    "text": "tweets per day",
    "style": "{color: #736AFF; font-size: 12px;}"
  },
 
   "x_legend":{
     "text": "date",
     "style": "{color: #736AFF; font-size: 12px;}"
   },
 
  "elements":[
    {
      "type": "area_line",
      "fill-alpha": 0.7,
      "colour":    "#9933CC",
      "text":      "Page views",
      "font-size": 10,
      "values" :   [<%= @values %>]
    }
  ],
 
  "x_axis":{
    "stroke":1,
    "tick_height":10,
    "colour":"#d000d0",
    "grid_colour":"#00ff00",
    "labels": { 
      "steps": 2,
      "rotate": "vertical",    
      "labels" : [<%= @keys %>]
    }
   },
 
  "y_axis":{
    "stroke":      4,
    "tick_length": 3,
    "colour":      "#d000d0",
    "grid_colour": "#00ff00",
    "offset":      0,
    "max":         10
  }
}
 
